<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.brandon.transblink">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TripPlanner" />
        <activity android:name=".StationMap" />
        <activity android:name=".LocationStation" />
        <activity android:name=".AboutUs" />
        <activity android:name=".ChangeLog" />
        <activity android:name=".ThemeChange" />
        <activity android:name=".StationInformation"></activity>
    </application>

</manifest>